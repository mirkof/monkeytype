{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [],
    "rewrites": [{
      "source": "**",
      "destination": "/soon.html"
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
