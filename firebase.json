{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [{
      "source": "/",
      "destination": "/app",
      "type": 301
    }],
    "rewrites": [{
      "source": "/**",
      "destination": "/app"
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
